-- Scripts:

local function LPPHW_fake_script() -- ScriptHubButton.LocalScript 
	local script = Instance.new('LocalScript', ScriptHubButton)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.ScriptFrame.Visible = true
		script.Parent.Parent.Parent.InfoFrame.Visible = false
		script.Parent.Parent.Parent.UpdateLogFrame.Visible = false
	end)
end
coroutine.wrap(LPPHW_fake_script)()
local function TKCZQS_fake_script() -- UpdateLogButton.LocalScript 
	local script = Instance.new('LocalScript', UpdateLogButton)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.ScriptFrame.Visible = false
		script.Parent.Parent.Parent.InfoFrame.Visible = false
		script.Parent.Parent.Parent.UpdateLogFrame.Visible = true
	end)
end
coroutine.wrap(TKCZQS_fake_script)()
local function QCQLNVJ_fake_script() -- InfoButton.LocalScript 
	local script = Instance.new('LocalScript', InfoButton)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.ScriptFrame.Visible = false
		script.Parent.Parent.Parent.InfoFrame.Visible = true
		script.Parent.Parent.Parent.UpdateLogFrame.Visible = false
	end)
end
coroutine.wrap(QCQLNVJ_fake_script)()
local function RKRNBF_fake_script() -- DaHoodButton.LocalScript 
	local script = Instance.new('LocalScript', DaHoodButton)

	script.Parent.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet('https://raw.githubusercontent.com/Scrvpter/Pluto/Lua/Loader.Lua', true))()
	end)
end
coroutine.wrap(RKRNBF_fake_script)()
