local function CZKEUN_fake_script() -- MainFrame.LocalScript 
	local script = Instance.new('LocalScript', MainFrame)

	local MainFrame = script.Parent.Parent.MainFrame --in the .frame - you should need to change it what is your frame name.
	local hotkey = Enum.KeyCode.K --Change your keybind you want.
	
	local UIS = game:GetService("UserInputService")
	local open = false
	
	
	
	UIS.InputBegan:Connect(function(key, gp)
		if key.KeyCode == hotkey then
			if UIS:GetFocusedTextBox() == nil then
				if open == false then
					open = true 
					MainFrame.Visible = open
				elseif open == true then
					open = false
					MainFrame.Visible = open
				end
			end
		end
	end)
end
coroutine.wrap(CZKEUN_fake_script)()
